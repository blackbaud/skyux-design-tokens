{
  "margin": {
    "inline": {
      "xs": {
        "$value": "{space.xs}",
        "$extensions": {
          "sky.class": "sky-margin-inline-xs"
        }
      },
      "sm": {
        "$value": "{space.sm}",
        "$extensions": {
          "sky.class": "sky-margin-inline-sm"
        } },
      "md": {
        "$value": "{space.md}",
        "$extensions": {
          "sky.class": "sky-margin-inline-md"
        } },
      "lg": {
        "$value": "{space.lg}",
        "$extensions": {
          "sky.class": "sky-margin-inline-lg"
        } },
      "xl": {
        "$value": "{space.xl}",
        "$extensions": {
          "sky.class": "sky-margin-inline-xl"
        } },
      "xxl": {
        "$value": "{space.xxl}",
        "$extensions": {
          "sky.class": "sky-margin-inline-xxl"
        } }
    }
  },
  "padding": {
    "button": {
      "standard": {
        "$value": "8px 20px"
      },
      "icon": {
        "$value": "8px 14px"
      }
    },
    "even": {
      "sm": {
        "$value": "{space.sm} {space.sm} {space.sm} {space.sm}"
      }
    }
  }
}
